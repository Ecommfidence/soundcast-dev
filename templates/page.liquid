{% elsif page.handle == 'press-images' %}

<div id="page-body">
	<div class="container">
		<h1 class="sectionz-title black">Soundcast Photos & Logos</h1>
	</div>

	<div ng-controller="pressCtrl" ng-cloak>
		<div class="container">
		<div class="row">
		{% raw %}
		<div id="press-item" ng-repeat="item in items" class="col-sm-3 nopadding text-center">
			<img src="{{item.image}}" class="img-thumbnail" alt="">

			<br>
			<p class="text-letter">{{ item.title }}</p>
			    <div id="pressdl" class="row text-center hidden-xs">
		            <div class="col-sm-6 text-right">
		                <a href="{{item.low}}" target="_blank" class=""><i class="fa fa-cloud-download"></i> Lo-Res<small></small></a>
		            </div>
		            <div class="col-sm-6 text-left">
		                <a href="{{item.high}}" target="_blank" class=""><i class="fa fa-cloud-download"></i> Hi-Res<small></small></a>
		            </div>
		        </div>
		        <div id="pressdl" class="row text-center visible-xs">
		            <div class="col-xs-6">
		                <a href="{{item.low}}" target="_blank" class=""><i class="fa fa-cloud-download"></i> Lo-Res<small></small></a>
		            </div>
		            <div class="col-xs-6">
		                <a href="{{item.high}}" target="_blank" class=""><i class="fa fa-cloud-download"></i> Hi-Res<small></small></a>
		            </div>
		        </div>
		<br>
		</div>

		{% endraw %}
		</div>
		</div>
	</div>
</div>

