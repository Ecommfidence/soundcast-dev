{% elsif page.handle == 'forgot-password' %}



<div id="page-body">
    <div class="page-title">
    <h1>Forgot Your Password?</h1>
</div>
<div id="password_forgot" class="container">
<hr>

    <p id="removez" class="reset-success success hidden">Instructions for resetting your password has been sent.</p>

<div class="retrieve">
    {% form 'recover_customer_password' %}
      {{ form.errors | default_errors }}

      {% if form.posted_successfully? %}
      <p class="success_pw">An email has been sent containing password reset instructions.</p>
      {% endif %}
    <div class="fieldset col-xs-12 text-center">
        <h2 class="legend">Retrieve your password here</h2>
        <p>Please enter your email address below. You will receive a link to reset your password.</p>
        <ul class="form-list">
            <li class="row">
                <label for="email_address" class="required col-xs-4 text-right email_label_login"><em>*</em>Email Address</label>
                <div class="input-box col-xs-8 text-left">
                    <input type="text" name="email" alt="email" id="recover-email" class="input-text required-entry validate-email" value="" />
                </div>
            </li>
                    </ul>
    </div>
    <div class="buttons-set text-center">
        <p class="required text-center">* Required Fields</p>
        <p class="back-link text-center"><a href="/account/login/"><small>&laquo; </small>Back to Login</a></p>
        <button type="submit" title="Submit" class="btn btn-default msgs"><span><span>Submit</span></span></button>
    </div>
  {% endform %}
</div>
</div>

